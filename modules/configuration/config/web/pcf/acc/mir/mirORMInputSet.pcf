<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <InputSet
    editable="true"
    id="MirORMInputSet">
    <Require
      name="anMIRReportable__Acc"
      type="MIRReportable_Acc"/>
    <BooleanRadioInput
      editable="true"
      id="HasOrm"
      label="DisplayKey.get(&quot;Accelerator.mir.label.orm&quot;)"
      required="true"
      value="anMIRReportable__Acc.HasORM"
      visible="true"/>
    <DateInput
      editable="true"
      id="OrmTermDate"
      label="DisplayKey.get(&quot;Accelerator.mir.label.ormterm&quot;)"
      required="anMIRReportable__Acc.HasORM == Boolean.TRUE"
      value="anMIRReportable__Acc.ORMTermDate"/>
  </InputSet>
</PCF>